<Window x:Class="LevelSetsEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LevelSetsEditor"
        xmlns:vm ="clr-namespace:LevelSetsEditor.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" >
    <!--<Window.DataContext>
        <vm:>
    </Window.DataContext>-->
    <Grid>
        <ListBox ItemsSource="{Binding Test, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" 
                 Height="403" Margin="10,10,0,0" VerticalAlignment="Top" Width="219">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <Label Content="{Binding }"></Label>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
        <Label Content="{Binding Name}" HorizontalAlignment="Left" Margin="267,22,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
