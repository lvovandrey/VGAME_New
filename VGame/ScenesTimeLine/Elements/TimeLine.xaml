<UserControl x:Class="ScenesTimeLine.Elements.TimeLine"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ScenesTimeLine.Elements"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="800">
    <Grid x:Name="GridMain">
        <local:ScaleTime x:Name="T1"></local:ScaleTime>
        <local:ScaleTime x:Name="T2"></local:ScaleTime>
        <local:Cursor x:Name="Cursor1" Margin="100,-5,0,-5" HorizontalAlignment="Left" Panel.ZIndex="10" Width="13">
            <local:Cursor.Effect>
                <DropShadowEffect BlurRadius="0" ShadowDepth="1" Direction="315" Color="White"/>
            </local:Cursor.Effect>


        </local:Cursor>
    </Grid>
</UserControl>
