<UserControl x:Class="ScenesTimeLine.Elements.ScenesTimeLine"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ScenesTimeLine.Elements"
             mc:Ignorable="d" 
             d:DesignHeight="45" d:DesignWidth="450">
    <StackPanel Name="MainStack" Orientation="Horizontal"  MouseUp="MainStack_MouseUp">
        <StackPanel.Background>
            <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                <GradientStop Color="#330000FF" Offset="0.124"/>
                <GradientStop Color="#349F9FFF" Offset="0.853"/>
            </LinearGradientBrush>
        </StackPanel.Background>
        

    </StackPanel>
</UserControl>
